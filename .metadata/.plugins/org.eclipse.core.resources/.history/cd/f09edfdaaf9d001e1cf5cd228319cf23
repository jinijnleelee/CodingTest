package level0;

public class Test1 {
	//1
	// 정수 num1과 num2가 주어질 때, 
	//num1에서 num2를 뺀 값을 return하도록 soltuion 함수를 완성해주세요.
	
	public static  int solution1(int num1, int num2) {
	    
	    int answer = 0;
	    
	    //답  
	    answer = num1 - num2 ;
	    return answer;
	    
	   
	}
	//2
	//정수 num1, num2가 매개변수로 주어질 때, 
	//num1을 num2로 나눈 몫을 return 하도록 solution 함수를 완성해주세요.
	
	public static int solution2(int num1, int num2) {
	    int answer = 0;
	    
	    //답  
	    answer = num1 / num2;
	    return answer;
	}
	//3
	//정수 num1, num2가 매개변수 주어집니다. 
	//num1과 num2를 곱한 값을 return 하도록 solution 함수를 완성해주세요.
	public static int solution3(int num1, int num2) {
	    int answer = 0;
	    
	    //답 
	    answer =num1*num2;
	    return answer;
	}
	//4
	//정수 num1, num2가 매개변수로 주어질 때, 
	//num1를 num2로 나눈 나머지를 return 하도록 solution 함수를 완성해주세요.
	public static int solution4(int num1, int num2) {
	    int answer = -1;
	    
	    //답 
	    answer =  num1 % num2;
	    return answer;
	}
	//5
	//정수 num1과 num2가 주어질 때, 
	//num1과 num2의 합을 return하도록 soltuion 함수를 완성해주세요.
	public static int solution5(int num1, int num2) {
	    int answer = -1;
	    
	    //답 
	    answer =  num1 + num2;
	    return answer;
	}	
	//6
	//머쓱이는 40살인 선생님이 몇 년도에 태어났는지 궁금해졌습니다. 나이 age가 주어질 때, 
	//2022년을 기준 출생 연도를 return 하는 solution 함수를 완성해주세요.
	
	public static  int solution6(int age) {
	   
	    int answer = 0;
	    
	    //답
	    answer = 2022-(age-1);

	    return answer;
	}
	//7
	//정수 num1과 num2가 매개변수로 주어질 때, num1을 num2로 나눈 값에 1,000을 곱한 후 
	//정수 부분을 return 하도록 soltuion 함수를 완성해주세요.
	
	public static  int solution7(int num1, int num2) {
	   
        int answer = 0;
        
        double div  = 0;
        div = (double)num1 /(double)num2 ;

        answer +=  div * 1000; 
    
    
        return  answer ;
	}	    
	  
	public static  int solution8(int angle) {
        int answer = 0;
//        if(angle == 180){
//        	   answer =4;
//        }else if(angle < 180 ) {
//        	if(90 < angle)
//        	   answer =3;
//        }else if(angle == 90) {
//        	   answer =2;
//        }else {
//        	  answer =1;
//        }
        
        if(0 < angle){
            answer =1;
        }else if(angle == 90){
            answer =2;
        }else if(90 < angle ){
        	if(angle< 180)
         	   answer =3;
       
        }else{
            answer =4;   
        }
        
            
        return answer;
    
	}	

	public static void main(String[] args) {
		
		
		System.out.println("solution1 "+solution1(+2,1));
		System.out.println("solution2 "+solution2(+2,1));
		System.out.println("solution3 "+solution3(+2,1));
		System.out.println("solution4 "+solution4(+2,1));
		System.out.println("solution5 "+solution5(+2,1));
		System.out.println("solution8 "+solution8(70));
		
		
		

	}

}
